{
  "name": "Example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "npx jest test/*.spec.js --runInBand",
    "lint": "eslint ."
  },
  "dependencies": {
    "axios": "0.19.2",
    "react": "16.11.0",
    "react-native": "0.62.3",
    "supertokens-react-native": "git+https://github.com/supertokens/supertokens-react-native.git#1.2"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/runtime": "7.9.2",
    "@react-native-community/eslint-config": "1.0.0",
    "babel-jest": "25.2.3",
    "eslint": "6.8.0",
    "jest": "25.2.3",
    "metro-react-native-babel-preset": "0.59.0",
    "react-test-renderer": "16.11.0",
    "babel-preset-react-app": "7.0.0",
    "ts-jest": "25.3.0",
    "node-fetch": "2.6.0",
    "fetch-cookie": "0.8.0",
    "axios-cookiejar-support": "0.5.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./test/setup.js"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.*\\.(js|jsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/.+\\.(js | ts | jsx | tsx)"
    ]
  }
}
